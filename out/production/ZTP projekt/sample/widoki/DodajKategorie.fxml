<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.kontrolery.DodajKategorieKontroler">
   <children>
      <HBox alignment="TOP_CENTER" spacing="20.0">
         <children>
            <Label text="Nowa Kategoria" />
            <TextField fx:id="kategoriaTextField" />
            <Button fx:id="dodajKategorieButton" mnemonicParsing="false" onAction="#dodajKategorie" text="Dodaj" />
            <Label text="kategoriaLabel" />
            <ComboBox fx:id="kategoriaComboBox" prefWidth="150.0" />
            <Button fx:id="usunKategorieButton" mnemonicParsing="false" onAction="#usunKategorie" text="Usun" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
